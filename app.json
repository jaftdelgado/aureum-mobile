{
  "expo": {
    "name": "aureum-mobile",
    "slug": "aureum-mobile",
    "owner": "jaftdelgado",
    "scheme": "aureum",
    "version": "1.0.0",
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": ["expo-font"],
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jaftdelgado.aureum",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "extra": {
      "eas": {
        "projectId": "6ee985c4-cbdf-4b13-acf1-fa8116388523"
      }
    }
  }
}
