{
  "expo": {
    "name": "aureum-mobile",
    "slug": "aureum-mobile",
    "owner": "jaftdelgado",
    "scheme": "aureum",
    "version": "1.0.0",
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "La aplicación necesita acceso a tus fotos para subir la imagen del curso o perfil.",
          "cameraPermission": "La aplicación necesita acceso a la cámara para tomar fotos de perfil."
        }
      ]
    ],
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jaftdelgado.aureum",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.jaftdelgado.aureum",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "extra": {
      "eas": {
        "projectId": "6ee985c4-cbdf-4b13-acf1-fa8116388523"
      }
    }
  }
}
